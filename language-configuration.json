{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "\"", "close": "\"" }
    ],
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["\"", "\""]
    ],
    "folding": {
      "markers": {
        "start": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*.*:\\s*$",
        "end": "^\\s*$"
      }
    }
}
